            case 6:
            {
                url = url + '/reports/ch/cs/' + txtReport_tmp.text + '?l_dept=' + lstDept_Code_6.value  + '&l_group=' + lstGrp_Code_6.value + '&l_status=' + lstStatus_6.value + '&l_emp_id=' + txtEmp_ID_6.text + '&l_work_mon=' + dtMonth.value + '&l_pay_type=' + lstBank_6.value + '&l_type=' + lstType_6.value + '&l_m_type=' + lstMoney_Type_6.value;
