                break;
            }
            case 5:
            {
                url = url + '/reports/ch/cs/' + txtReport_tmp.text + '?l_dept=' + lstDept_Code_5.value  + '&l_group=' + lstGrp_Code_5.value + '&l_status=' + lstStatus_5.value + '&l_emp_id=' + txtEmp_ID_5.text + '&l_work_mon=' + dtMonth.value + '&l_pay_type=' + lstPay_Type_5.value + '&l_type=' + lstType_5.value + '&l_m_type=' + lstMoney_Type_5.value;
                break;
